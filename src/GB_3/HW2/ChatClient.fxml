<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HW8.ChatController">
   <BorderPane prefHeight="342.0" prefWidth="553.0">
      <center>
          <TextArea fx:id="messageArea" editable="false" focusTraversable="false" prefHeight="135.0" prefWidth="42.0" BorderPane.alignment="CENTER" />
      </center>
      <left>
         <ListView fx:id="nickList" prefHeight="315.0" prefWidth="124.0" BorderPane.alignment="CENTER" />
      </left>
      <bottom>
         <FlowPane prefHeight="27.0" prefWidth="241.0" BorderPane.alignment="CENTER">
            <children>
                <TextField fx:id="newMessage" onAction="#sendMessageTypeAction" prefHeight="26.0" prefWidth="492.0" />
                <Button fx:id="sendButton" onAction="#sendMessageTypeAction" text="Send" />
            </children>
         </FlowPane>
      </bottom>
      <top>
         <FlowPane prefHeight="35.0" prefWidth="553.0" BorderPane.alignment="CENTER">
            <children>
               <TextField fx:id="Nick" prefHeight="21.0" prefWidth="149.0" />
               <Button fx:id="ChangeNick" onAction="#ChangeNick" mnemonicParsing="false" prefHeight="25.0" prefWidth="95.0" text="Change nick" />
            </children>
         </FlowPane>
      </top>
   </BorderPane>
</AnchorPane>
